{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "glacier:ListVaults",
                "glacier:ListJobs",
                "glacier:InitiateJob",
                "glacier:DescribeJob",
                "glacier:GetJobOutput",
                "glacier:UploadArchive",
                "glacier:DeleteArchive",
                "glacier:DeleteVault",
                "glacier:DescribeVault",
                "glacier:GetDataRetrievalPolicy",
                "glacier:SetDataRetrievalPolicy"
            ],
            "Resource": "*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "elasticache:Describe*",
                "elasticache:List*",
                "elasticache:Access*",
                "dynamodb:DescribeTable",
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem",
                "dynamodb:DeleteItem"
            ],
            "Resource": "*"
        }
    ]
}
